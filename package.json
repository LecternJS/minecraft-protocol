{
  "name": "minecraft-protocol",
  "version": "2.0.0",
  "description": "Parse/Serialize Minecraft Packets with Microsoft Authentication and Encryption",
  "main": "src/index.js",
  "module": "./src/index.mjs",
  "repository": "https://github.com/LecternJS/minecraft-protocol.git",
  "author": "Jordan Jones <jordpjones@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "debug": "^4.3.1",
    "discord.js-docgen": "github:discordjs/docgen",
    "eslint": "^7.17.0",
    "eslint-config-kashall": "https://github.com/Kashalls/kashall-lint.git",
    "minecraft-packets": "^1.3.0",
    "minecraft-wrap": "^1.3.0",
    "mocha": "^9.0.1",
    "power-assert": "^1.6.1"
  },
  "exports": {
    "require": "./src/index.js",
    "import": "./src/index.mjs"
  },
  "scripts": {
    "test": "npm run lint && npm run docs:test && npm run test:proto",
    "test:proto": "mocha --recursive --reporter spec --exit --exclude \"non-par-test.js\"",
    "test:proto-non-par": "mocha --recursive --reporter spec --exit \"test/non-par-test.js\"",
    "lint": "eslint .",
    "docs": "docgen --source src --custom docs/index.yml --output docs/docs.json",
    "docs:test": "docgen --source src --custom docs/index.yml",
    "prepublishOnly": "npm run test && gen-esm-wrapper ./src/index.js ./src/index.mjs"
  },
  "dependencies": {
    "@azure/msal-node": "^1.0.0-beta.3",
    "@xboxreplay/xboxlive-auth": "^3.3.3",
    "aes-js": "^3.1.2",
    "fs-nextra": "^0.5.1",
    "minecraft-data": "^2.85.3",
    "node-fetch": "^2.6.1",
    "node-rsa": "^1.1.1",
    "prismarine-nbt": "^1.5.0",
    "protodef": "^1.8.3",
    "uuid-1345": "^1.0.2",
    "yggdrasil": "^1.4.0"
  }
}
